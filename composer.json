{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/burki/SolrBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/OaiPmh"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jtl-software/connector-iso-639.git"
        }
    ],
    "require": {
        "php": "^7.4|^8.1",
        "ext-ctype": "*",
        "ext-exif": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "armin/scssphp-bundle": "^1.2",
        "boppy/solr-bundle": "dev-solarium-6",
        "doctrine/annotations": "^1.14",
        "edwinhoksberg/octicons-php": "^1.1",
        "flagception/flagception-bundle": "^4.3",
        "fluentdom/fluentdom": "^8.0",
        "gmo/iso-639": "dev-master#4834357",
        "jms/translation-bundle": "^1.7",
        "knplabs/knp-menu-bundle": "^3.2",
        "knplabs/knp-paginator-bundle": "^5.9",
        "laminas/laminas-diactoros": "^2.13",
        "masterminds/html5": "^2.8",
        "mpdf/mpdf": "^8.1",
        "nyholm/psr7": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.2",
        "presta/sitemap-bundle": "^3.3",
        "sabre/xml": "^1.5",
        "saidatom/oai-pmh": "dev-master",
        "seboettg/citeproc-php": "^2.6",
        "spatie/schema-org": "^3.4",
        "sylius/theme-bundle": "^2.1",
        "symfony/asset": "^5.4",
        "symfony/console": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/dom-crawler": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/expression-language": "^5.4",
        "symfony/flex": "^1.17 || ^2.0",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/intl": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-intl-messageformatter": "^1.27",
        "symfony/process": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/psr-http-message-bridge": "^2.0",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/string": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/web-link": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/extra-bundle": "^3.6",
        "twig/string-extra": "^3.6",
        "twig/twig": "^3.6",
        "voku/html-min": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/stopwatch": "^5.4",
        "symfony/web-profiler-bundle": "^5.4",
        "vimeo/psalm": "^4.2"
    },
    "config": {
        "platform": {
            "php": "7.4.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
